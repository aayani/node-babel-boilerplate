{
  "name": "node-babel-boilerplate",
  "author": "Wahaj Aayani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all clean compile",
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist",
    "lint": "eslint src/* __tests__/* --quiet --fix",
    "start": "node -r dotenv/config bin/www",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node -r dotenv/config bin/www",
    "start:prod": "cross-env NODE_ENV=production npm-run-all build start",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@hapi/joi": "^17.1.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.12.0",
    "koa-router": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@types/jest": "^25.2.3",
    "babel-eslint": "^10.1.0",
    "babel-minify": "^0.5.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  }
}
