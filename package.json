{
  "name": "node-babel-boilerplate",
  "author": "Wahaj Aayani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all clean compile",
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist",
    "format": "eslint src* --fix",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/main.js",
    "start:prod": "cross-env NODE_ENV=production npm-run-all clean compile start"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-minify": "^0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  }
}
