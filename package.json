{
  "name": "node-babel-boilerplate",
  "author": "Wahaj Aayani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all clean compile",
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist",
    "lint": "eslint src* --quiet --fix",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node -r dotenv/config src/main.js",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config dist/main.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.26",
    "dotenv": "^8.2.0",
    "http-status-codes": "^1.4.0",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-minify": "^0.5.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  }
}
