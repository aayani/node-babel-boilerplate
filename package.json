{
  "name": "node-babel-boilerplate",
  "author": "Wahaj Aayani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm-run-all clean compile",
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist",
    "lint": "eslint src/* __tests__/* --quiet --fix",
    "start": "node -r dotenv/config bin/www",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node -r dotenv/config bin/www",
    "start:prod": "cross-env NODE_ENV=production npm-run-all build start",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "dayjs": "^1.9.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-router": "^10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@types/jest": "^26.0.15",
    "babel-minify": "^0.5.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1"
  }
}
